  </servlet>
  <servlet-mapping>
  	<servlet-name>ps</servlet-name>
  	<url-pattern>/ps</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>gs</servlet-name>
  	<servlet-class>com.sessionAndCookie.getSession</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>gs</servlet-name>
  	<url-pattern>/gs</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>gc</servlet-name>
  	<servlet-class>com.sessionAndCookie.getCookie</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>gc</servlet-name>
  	<url-pattern>/gc</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>pc</servlet-name>
  	<servlet-class>com.sessionAndCookie.postCookie</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>pc</servlet-name>
  	<url-pattern>/pc</url-pattern>
  </servlet-mapping>
</web-app>